import math.tomb;

let mut counter: int = 0;

export func main() -> unit {
    let pi: float = 3.14;

    if pi > 3.0 {
        print("Pi is large");
    } else {
        print("Pi is small");
    }

    while counter < 10 {
        counter = counter + 1;
    }

    for i: int = 0; i < 5; i = i + 1 {
        print(i);
    }
}